{
  "privileges": [],
  "dataRoles": [
    {
      "name": "category",
      "kind": "Grouping",
      "displayName": "Hierarchy"
    },
    {
      "name": "value",
      "kind": "Measure",
      "displayName": "Value"
    }
  ],
  "dataViewMappings": [
    {
